{
    "name": "reedware/laravel-blueprints",
    "description": "Adds useful methods to the blueprint schema builder.",
    "keywords": [
        "laravel", "database", "schema", "blueprint"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Reed",
            "email": "tylernathanreed@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/database": "^5.1|^6.0|^7.0",
        "illuminate/support": "^5.1|^6.0|^7.0"
    },
    "require-dev": {
        "ext-fileinfo": "*",
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\LaravelBlueprints\\": "src/"
        }
    },
    "autoload-dev": {},
    "suggest": {},
    "conflict": {},
    "config": {},
    "extra": {
        "laravel": {
            "providers": [
                "Reedware\\LaravelBlueprints\\BlueprintsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
